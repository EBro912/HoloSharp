<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>preview</LangVersion>
    <Authors>EBro912</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>A C# Wrapper for the HoloTools API</Description>
    <Copyright>Copyright 2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/EBro912/HoloSharp</PackageProjectUrl>
    <PackageIcon>logo.png</PackageIcon>
    <PackageTags>hololive holotools wrapper</PackageTags>
    <PackageReleaseNotes>- Fixed Stream Thumbnail getter, now automatically parses into a URL since the HoloTools thumbnail object is now unused.
- Made all Video elements nullable, since the HoloTools API will return null values for videos marked as missing</PackageReleaseNotes>
    <Version>1.2.1</Version>
    <AssemblyVersion>1.2.1.0</AssemblyVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>1701;1702;1591</NoWarn>
  </PropertyGroup>

  
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

  
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  
  <ItemGroup>
    <None Include="logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>